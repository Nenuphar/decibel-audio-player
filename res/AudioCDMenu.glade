<?xml version="1.0"?>
<glade-interface>
<widget class="GtkMenu" id="menu-popup">
<child>
<widget class="GtkImageMenuItem" id="item-play">
<property name="label">gtk-media-play</property>
<property name="tooltip" translatable="yes">Replace the playlist by the selection and start playback</property>
<property name="use_underline">True</property>
<property name="use_stock">True</property>
</widget>
</child>
<child>
<widget class="GtkImageMenuItem" id="item-add">
<property name="label">Add to Playlist</property>
<property name="tooltip" translatable="yes">Append selection to the playlist</property>
<property name="use_underline">True</property>
<property name="use_stock">True</property>
</widget>
</child>
<child>
<widget class="GtkSeparatorMenuItem" id="menuitem1"/>
</child>
<child>
<widget class="GtkImageMenuItem" id="item-refresh">
<property name="label">gtk-refresh</property>
<property name="tooltip" translatable="yes">Rescan the audio disc</property>
<property name="use_underline">True</property>
<property name="use_stock">True</property>
</widget>
</child>
</widget>
</glade-interface>
